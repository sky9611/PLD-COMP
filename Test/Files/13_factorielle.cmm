#include<inttypes.h>
int64_t getInt(){
	char c,t;
    int32_t test = 0;

	int64_t res = 0;
	c = getchar();
    test = (c >= '0' && c <= '9');
	while(test){
        putchar(c);
        putchar(':');
        putchar('0'+test);
        putchar(' ');
		res *= 10;
		res += c - '0';
		c = getchar();
        test = (c >= '0' && c <= '9');
        putchar('0'+test);
        putchar('\n');
        res = res;
	}
    return res;
	
}
void putInt(int64_t value){
    char c;
    int64_t i=1;
    while(i<value){
        i*=10;
    }
    i/=10;
    while(i>0){
        c = value/i + '0';
        putchar(c);
        i/=10;
    }
}

int64_t factorielle (int64_t nombre)
{
    int64_t x;
    x = 0;
    if (nombre == 1)
    {
        x = 1;
        return(x);
    }
    else
    {
        x = nombre * factorielle(nombre - 1);
        return(x);
    }
}
 
