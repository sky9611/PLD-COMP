#include<inttypes.h>
void putInt(int64_t value){
    char c;
    int64_t i=1;
    while(i<value){
        i*=10;
    }
    i/=10;
    while(i>0){
        c = value/i + '0';
        putchar(c);
        i/=10;
    }
}

int64_t factorielle (int64_t nombre)
{
    int64_t x;
    x = 0;
    if (nombre == 1)
    {
        x = 1;
        return(x);
    }
    else
    {
        x = nombre * factorielle(nombre - 1);
        return(x);
    }
}
 
void main(void){
    int64_t n = 5, resultat = 0;

    resultat = factorielle(n);
    putInt(resultat);
    return 0;
}
